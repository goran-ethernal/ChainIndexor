{
  "downloader": {
    "rpc_url": "https://mainnet.infura.io/v3/XXXX",
    "chunk_size": 5000,
    "finality": "finalized",
    "finalized_lag": 12,
    "db": {
      "path": "./data/downloader.sqlite",
      "journal_mode": "WAL",
      "synchronous": "NORMAL",
      "busy_timeout": 5000,
      "cache_size": 10000,
      "max_open_connections": 25,
      "max_idle_connections": 5,
      "enable_foreign_keys": true
    },
    "retention_policy": {
      "max_db_size_mb": 1000,
      "max_blocks": 10000
    },
    "maintenance": {
      "enabled": true,
      "check_interval": "5m",
      "vacuum_on_startup": true
    }
  },
  "indexers": [
    {
      "name": "MyTokenIndexer",
      "start_block": 17000000,
      "db": {
        "path": "./data/mytokenindexer.sqlite",
        "journal_mode": "WAL",
        "synchronous": "NORMAL",
        "busy_timeout": 5000,
        "cache_size": 10000,
        "max_open_connections": 25,
        "max_idle_connections": 5,
        "enable_foreign_keys": true
      },
      "contracts": [
        {
          "address": "0x1234567890abcdef1234567890abcdef12345678",
          "events": [
            "Transfer(address,address,uint256)",
            "Approval(address,address,uint256)"
          ]
        }
      ]
    }
  ]
}
